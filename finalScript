#include <iostream>
#include <string>
using namespace std;
int main()
{
	string firstName;
	string lastName;
	string ID;
	bool inputDone = false;
	do {
		cout << "Please enter Firstname and Lastname" << endl;
		cin >> firstName >> lastName >> ID;
		for (int i = 1; ID.length(); i++)
			if (static_cast<int>(ID[i]) <= 9 || static_cast<int>(ID[i]) >= 0)
				inputDone = false;
				
	} while (inputDone);
	

	return 0;
}
