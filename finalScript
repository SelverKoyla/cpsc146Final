#include <iostream>
#include <string>
using namespace std;
int main()
{
	string firstName;
	string lastName;
	string ID;
	bool inputDone;
	do {
		cout << "Please enter Firstname and Lastname" << endl;
		
		for (int i = 1; ID.length; i++)
			if (static_cast<int>(ID[i]) <= 9 || static_cast<int>(ID[i]) >= 0)
				inputDone = false;
				
	} while (inputDone);
	cin >> firstName >> lastName >> ID;

	return 0;
}
